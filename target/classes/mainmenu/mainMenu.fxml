<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainMenuView">
<stylesheets>
    <URL value="@mainMenu.css" />
</stylesheets>
   <children>
      <Label fx:id="title" alignment="CENTER" layoutX="405.0" layoutY="10.0" prefHeight="42.0" prefWidth="190.0" text="Main Menu" />
      <Button fx:id="newGameButton" layoutX="90.0" layoutY="114.0" mnemonicParsing="false" onMouseClicked="#newGameClicked" prefHeight="50.0" prefWidth="127.0" text="new game" />
      <Button fx:id="accountButton" layoutX="784.0" layoutY="114.0" mnemonicParsing="false" onMouseClicked="#accountClicked" prefHeight="50.0" prefWidth="127.0" text="account" />
      <Button fx:id="scoreBoardButton" layoutX="90.0" layoutY="221.0" mnemonicParsing="false" onMouseClicked="#scoreBoardClicked" prefHeight="50.0" prefWidth="127.0" text="scoreboard" />
      <Button fx:id="exitButton" layoutX="784.0" layoutY="221.0" mnemonicParsing="false" onMouseClicked="#exitClicked" prefHeight="50.0" prefWidth="127.0" text="exit" />
      <Label fx:id="errorLabel" alignment="CENTER" layoutX="352.0" layoutY="561.0" prefHeight="23.0" prefWidth="296.0" />
   </children>
</AnchorPane>
