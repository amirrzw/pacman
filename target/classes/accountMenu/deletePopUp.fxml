    <stylesheets>
        <URL value="@deletePopUp.css" />
    </stylesheets>
   <children>
      <Label layoutX="37.0" layoutY="19.0" prefHeight="38.0" prefWidth="309.0" text="Are you sure ?" />
      <Button layoutX="79.0" layoutY="123.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="70.0" text="yes" onMouseClicked="#deleteAccount" fx:id="yesButton" />
      <Button layoutX="203.0" layoutY="123.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="70.0" text="no" onMouseClicked="#cancelDeleteAccount" fx:id="noButton" />
   </children>

</AnchorPane>
