<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EntranceMenuView">

    <stylesheets>
        <URL value="@entrance.css" />
    </stylesheets>
   <center>
      <AnchorPane prefHeight="339.0" prefWidth="1000.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="registerButton" layoutX="65.0" layoutY="85.0" mnemonicParsing="false" onMouseClicked="#registerClicked" prefHeight="48.0" prefWidth="139.0" text="Register" />
            <Button fx:id="loginButton" layoutX="808.0" layoutY="85.0" mnemonicParsing="false" onMouseClicked="#loginClicked" prefHeight="48.0" prefWidth="139.0" text="Login" />
            <Button fx:id="exitButton" layoutX="808.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#exitClicked" prefHeight="48.0" prefWidth="139.0" text="Exit" />
            <Button fx:id="playAsGuestButton" layoutX="65.0" layoutY="220.0" mnemonicParsing="false" onMouseClicked="#playAsGuestClicked" prefHeight="48.0" prefWidth="139.0" text="play as guest" />
         </children>
      </AnchorPane>
   </center>

</BorderPane>
