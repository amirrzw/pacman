<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.game.SetHealthMenuView">
    <stylesheets>
        <URL value="@setHealthMenu.css" />
    </stylesheets>
   <children>
      <AnchorPane layoutX="395.0" layoutY="350.0" prefHeight="92.0" prefWidth="200.0">
         <children>
            <Button fx:id="increaseButton" layoutX="119.0" layoutY="12.0" mnemonicParsing="false" prefHeight="69.0" prefWidth="93.0" onMouseClicked="#increaseHealthClicked" />
            <Button fx:id="decreaseButton" layoutX="14.0" layoutY="12.0" mnemonicParsing="false" prefHeight="69.0" prefWidth="93.0" onMouseClicked="#decreaseHealthClicked" />
         </children>
      </AnchorPane>
      <Button layoutX="408.0" layoutY="455.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="200.0" text="Next" fx:id="nextButton" onMouseClicked="#nextClicked" />
      <Label layoutX="372.0" layoutY="306.0" prefHeight="29.0" prefWidth="111.0" text="Your health : " />
      <Button layoutX="408.0" layoutY="532.0" mnemonicParsing="false" prefHeight="53.0" prefWidth="200.0" text="Back" fx:id="backButton" onMouseClicked="#backClicked" />
      <Label layoutX="483.0" layoutY="306.0" prefHeight="29.0" prefWidth="18.0" text="2" fx:id="healthLabel" />
      <Label layoutX="508.0" layoutY="306.0" prefHeight="29.0" prefWidth="153.0" text="(Minimum health)" fx:id="healthInfoLabel" />
      <Label fx:id="title" alignment="CENTER" layoutX="444.0" layoutY="24.0" prefHeight="53.0" prefWidth="130.0" text="Set health" />
   </children>

</AnchorPane>
